{
  "name": "hebo-event-repository-dynamodb",
  "description": "Event Repository implementation for hebo-js that uses AWS DynamoDb",
  "version": "1.3.0",
  "author": "Steve Caldwell <scaldwell@gmail.com>",
  "ava": {
    "failFast": true,
    "verbose": true
  },
  "bugs": {
    "url": "https://github.com/stevecaldwell77/hebo-event-repository-dynamodb/issues",
    "email": "scaldwell@gmail.com"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "contributors": [
    "Steve Caldwell <scaldwell@gmail.com>"
  ],
  "dependencies": {
    "@aws/dynamodb-auto-marshaller": "^0.7.1",
    "@aws/dynamodb-expressions": "^0.7.3",
    "@aws/dynamodb-query-iterator": "^0.7.1",
    "auto-bind": "^2.1.1",
    "hebo-validation": "^6.0.0",
    "lodash": "^4.17.15",
    "memoizee": "^0.4.14"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "ava": "^2.4.0",
    "aws-sdk": "^2.546.0",
    "codecov": "^3.6.1",
    "cross-env": "^6.0.3",
    "dynalite": "^3.0.0",
    "eslint": "^6.5.1",
    "eslint-config-archereducation": "^4.3.1",
    "fixpack": "^2.3.1",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.2",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "prettier-config-archereducation": "^1.0.0",
    "remark-cli": "^7.0.0",
    "remark-preset-github": "^0.0.16",
    "shortid": "^2.2.15",
    "uuid": "^3.3.3"
  },
  "engines": {
    "node": ">=8.16.1"
  },
  "homepage": "https://github.com/stevecaldwell77/hebo-event-repository-dynamodb",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "aws",
    "cqrs",
    "dynamodb",
    "es",
    "hebo-js"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "yarn lint --fix",
      "git add"
    ],
    "*.md": [
      "remark . -qfo",
      "git add"
    ],
    "package.json": [
      "fixpack",
      "git add"
    ]
  },
  "main": "index.js",
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100,
    "reporter": [
      "lcov",
      "html",
      "text"
    ]
  },
  "peerDependencies": {
    "aws-sdk": "^2.546.0",
    "hebo": ">=6.0.0"
  },
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stevecaldwell77/hebo-event-repository-dynamodb"
  },
  "scripts": {
    "ava": "cross-env NODE_ENV=test ava",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "remark . -qfo && eslint .",
    "nyc": "cross-env NODE_ENV=test nyc ava",
    "test": "npm run lint && npm run ava",
    "test-coverage": "npm run lint && npm run nyc"
  }
}
